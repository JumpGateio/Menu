{
    "name":        "jumpgate/menu",
    "description": "A simple menu system.",
    "license":     "MIT",
    "authors":     [
        {
            "name":  "Riddles",
            "email": "riddles@dev-toolbox.com"
        }
    ],
    "require": {
        "illuminate/support": ">=4.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload":    {
        "files": [
            "src/helpers.php"
        ],
        "psr-4":    {
            "JumpGate\\Menu\\": "src/JumpGate/Menu"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JumpGate\\Menu\\MenuServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
